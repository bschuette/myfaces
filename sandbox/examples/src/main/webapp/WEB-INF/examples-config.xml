  	<managed-bean-scope>session</managed-bean-scope>
  		<managed-property>
  			<property-name>mode</property-name>
  			<value>1</value>
  		</managed-property>
  		<managed-property>
  			<property-name>selectedDate</property-name>
  			<value>#{today}</value>
